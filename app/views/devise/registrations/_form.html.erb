  <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= devise_error_messages! %>

<div class="row">
  <div class="col-sm-12">
    <div class="sweepSchTextLabels"><%= f.label :company_name %>
    <%= f.text_field :company_name, class: "form-control" %></div>

    <div class="sweepSchTextLabels"><%= f.label :email %>
    <%= f.email_field :email, class: "form-control" %></div>

    <div class="sweepSchTextLabels"><%= f.label :password %> <% if @validatable %><i>(<%= @minimum_password_length %> characters minimum)</i><% end %><br />
      <%= f.password_field :password, autocomplete: "off", class: "form-control" %></div>

    <div class="sweepSchTextLabels"><%= f.label :password_confirmation %>
      <%= f.password_field :password_confirmation, autocomplete: "off", class: "form-control" %></div>

    <div class="sweepSchSubmitWrapper col-sm-12"><%=f.submit "Create my account!", :class => 'sweepSchSubmit col-sm-4' %></div>
    <!--<%= render "devise/shared/links" %>-->
  </div>
</div>
  <% end %>

  
